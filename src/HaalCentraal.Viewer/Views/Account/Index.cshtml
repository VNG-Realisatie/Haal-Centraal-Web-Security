@model HaalCentraal.Viewer.Models.AccountViewModel
<div class="row">
    <table class="table text-break">
        <tr>
            <th class="w-25">Token</th>
            <th></th>
        </tr>
        @foreach (var key in Model.Tokens.Keys)
        {
            <tr>
                <td>@key</td>
                <td>@Model.Tokens[key]</td>
            </tr>
        }
    </table>
    <table class="table">
        <tr>
            <th>Claim</th>
            <th>value</th>
        </tr>
        @foreach (var key in Model.Claims.Keys)
        {
            <tr>
                <td>@key</td>
                <td>@Model.Claims[key]</td>
            </tr>
        }
    </table>
</div>
